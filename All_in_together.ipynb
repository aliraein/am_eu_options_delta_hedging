    "for d in arkq_stock[0:2]['Close'].index:\n",
    "    S = arkq_stock.loc[d, 'Close']\n",
    "\n",
    "    print('Stock Price S: ' + str(S))\n",
    "    K_c = S + 10\n",
    "    K_p = S - 10\n",
    "\n",
    "    r = rf.r(d, tao)\n",
    "    print('Risk Free Rate r: ' + str(r))\n",
    "\n",
    "    bs_call = European_Call(S, K_c, tao, r, sigma, div).get_Option_Price()\n",
    "    bs_put = European_Put(S, K_p, tao, r, sigma, div).get_Option_Price()\n",
    "\n",
    "    mc_call = MonteCarlo(n, steps, S, K_c, sigma, tao, r)\n",
    "    mc_put = MonteCarlo(n, steps, S, K_p, sigma, tao, r)\n",
    "    mc_call.simulate()\n",
    "    mc_put.simulate()\n",
    "    mc_call = mc_call.call\n",
    "    mc_put = mc_put.put\n",
    "\n",
    "    print('Black-Scholes call price at K = {0}: {1}'.format(str(K_c), str(bs_call)))\n",
    "    print('Monte-Carlo call price at K = {0}: {1}'.format(str(K_c), str(mc_call)))\n",
    "\n",
    "    print('Black-Scholes put price at K = {0}: {1}'.format(str(K_p), str(bs_put)))\n",
    "    print('Monte-Carlo put price at K = {0}: {1}'.format(str(K_p), str(mc_put)))"
   ]
